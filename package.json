{
  "name": "ajto-ablak",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "dev:ssr": "ng run ajto-ablak:serve-ssr",
    "serve:ssr": "node dist/ajto-ablak/server/main.js",
    "build:ssr": "ng build --prod --localize && ng run ajto-ablak:server:production",
    "postbuild:ssr": "npm run sw-prod-webpack",
    "build:prod": "ng build --prod --localize",
    "postbuild:prod": "npm run sw-prod-webpack && node helper/assetsUploader.js",
    "prerender": "ng run ajto-ablak:prerender",
    "start-hu": "ng serve --configuration=hu",
    "extract-i18n": "ng xi18n ajto-ablak --ivy --i18n-format xlf2 --output-path src/i18n --i18n-locale en && ng run ajto-ablak:xliffmerge",
    "sw-prod-webpack": "rimraf ./webpack-config/service-worker.js && webpack --config ./webpack-config/sw-prod-webpack.config.js --progress && workbox injectManifest ./workboxconfig/en.js && workbox injectManifest ./workboxconfig/hu.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.2.0",
    "@angular/cdk": "^10.2.7",
    "@angular/common": "~10.2.0",
    "@angular/compiler": "~10.2.0",
    "@angular/core": "~10.2.0",
    "@angular/forms": "~10.2.0",
    "@angular/material": "^10.2.7",
    "@angular/platform-browser": "~10.2.0",
    "@angular/platform-browser-dynamic": "~10.2.0",
    "@angular/platform-server": "~10.2.0",
    "@angular/router": "~10.2.0",
    "@angular/service-worker": "~10.2.0",
    "@nguniversal/express-engine": "^10.1.0",
    "@ngx-i18nsupport/tooling": "^8.0.3",
    "aws-sdk": "^2.916.0",
    "aws-serverless-express-edge": "^1.1.1",
    "component": "^1.1.0",
    "express": "^4.15.2",
    "jquery": "^3.6.0",
    "loader-utils": "^2.0.0",
    "localstorage-polyfill": "^1.0.1",
    "mock-browser": "^0.92.14",
    "ngx-google-analytics": "^11.2.0",
    "ngx-slick-carousel": "^0.5.1",
    "rxjs": "~6.6.0",
    "scriptjs": "^2.5.9",
    "serverless-plugin-scripts": "^1.0.2",
    "slick-carousel": "^1.8.1",
    "tslib": "^2.0.0",
    "uuid": "^8.3.2",
    "webpack": "^4.41.4",
    "webpack-latest": "npm:webpack@^5.36.1",
    "workbox-core": "^6.1.5",
    "workbox-precaching": "^6.1.5",
    "workbox-routing": "^6.1.5",
    "workbox-strategies": "^6.1.5",
    "workbox-window": "^6.1.5",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1002.0",
    "@angular/cli": "~10.2.0",
    "@angular/compiler-cli": "~10.2.0",
    "@angular/localize": "^10.2.4",
    "@nguniversal/builders": "^10.1.0",
    "@ngx-i18nsupport/ngx-i18nsupport": "^1.1.6",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/jquery": "^3.5.5",
    "@types/node": "^12.11.1",
    "@types/scriptjs": "0.0.2",
    "@types/uuid": "^8.3.0",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "rimraf": "^3.0.2",
    "ts-loader": "^8.2.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "^4.0.7",
    "webpack-cli": "^4.6.0",
    "workbox-cli": "^6.1.5"
  }
}
