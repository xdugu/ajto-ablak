{
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/manifest.webmanifest",
          "/*.css",
          "/*.js"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/*.(eot|svg|cur|jpg|jpeg|png|webp|gif|otf|ttf|woff|woff2|ani)"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "apis",
      "urls": ["https://3j1b7pw3nd.execute-api.eu-central-1.amazonaws.com/*"],
      "cacheConfig": {
        "maxSize": 50,
        "maxAge": "20m"
      }
    },
    {
      "name": "s3",
      "urls": ["https://adugu-common.s3.eu-central-1.amazonaws.com/*"],
      "cacheConfig": {
        "maxSize": 150,
        "maxAge": "30d"
      }
    }
  ]
}
